<style>
  /* GLOBAL */
  .container {
    max-width: 1160px;
    margin: 0 auto;
  }

  body {
    position: relative;
  }

  /* HERO */
  .hero {
    margin-top: 104px;
    text-align: center;
    padding: 150px 0;
  }

  .hero h1 {
    font-size: 70px;
    font-style: normal;
    font-weight: 800;
    line-height: normal;
    text-transform: capitalize;
  }

  .hero p {
    max-width: 70%;
    margin: 0 auto;
    font-size: 20px;
    font-style: normal;
    font-weight: 500;
    line-height: 30px; /* 150% */
  }

  .hero__arrow {
    margin-top: 80px;
  }

  .hero__line {
    position: absolute;
    top: 0;
    right: 0;
    z-index: -1;
    top: 5%;
  }

  /* ACCORDION */
  .accordion {
    width: 100%;
    margin: 0 auto;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    gap: 60px;
    padding: 150px 0;
  }

  .accordion-item {
    border-bottom: 1px solid #ccc;
    padding-bottom: 20px;
  }

  .accordion-item:last-child {
      border-bottom: none;
  }

  .accordion-header {
    width: 100%;
    padding: 0;
    text-align: right;
    border: none;
    outline: none;
    cursor: pointer;
    font-size: 16px;
    background: none;
    color: #fff;
  }

  .accordion-content {
      padding: 0;
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.2s ease-out;
  }

  .accordion-content p {
      margin: 15px 0;
  }

  .accordionContent__title {
    font-size: 34px;
    font-style: normal;
    font-weight: 800;
    line-height: normal;
    text-transform: capitalize;
  }

  .accordionContent__text {
    max-width: 60%;
    font-size: 20px;
    font-style: normal;
    font-weight: 500;
    line-height: normal;
  }

  .guidelines__itemTitle {
    font-size: 45px;
    font-style: normal;
    font-weight: 800;
    line-height: normal;
    text-transform: capitalize;
    margin: 0;
  }

  .guidelines__itemText {
    font-size: 20px;
    font-style: normal;
    font-weight: 500;
    line-height: normal;
    max-width: 60%;
  }

  .accordionItem__toggle {
    display: grid;
    grid-template-columns: auto auto;
  }

  .accordion__bottomText {
    font-family: roboto;
    text-align: center;
    font-size: 34px;
    font-style: normal;
    font-weight: 800;
    line-height: normal;
    max-width: 70%;
    margin: 0 auto;
    text-wrap: balance;
  }
</style>

